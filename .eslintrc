{
  "extends": [
    "@nuxt/eslint-config",
    "plugin:prettier/recommended",
    "plugin:tailwindcss/recommended"
  ],
  "settings": {
    "tailwindcss": {
      "whitelist": [
        ".*(primary).*"
      ]
    }
  },
  "rules": {
    "vue/multi-word-component-names": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "vue/valid-v-slot": "off"
  }
}
